<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    
    <servlet>
        <servlet-name>part1</servlet-name>
        <servlet-class>Business.part1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>part5</servlet-name>
        <servlet-class>Business.part5</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>part6</servlet-name>
        <servlet-class>Business.part6</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>part7</servlet-name>
        <servlet-class>Business.part7</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>part8</servlet-name>
        <servlet-class>Business.part8</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>part1</servlet-name>
        <url-pattern>/part1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>part5</servlet-name>
        <url-pattern>/part5</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>part6</servlet-name>
        <url-pattern>/part6</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>part7</servlet-name>
        <url-pattern>/part7</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>part8</servlet-name>
        <url-pattern>/part8</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
